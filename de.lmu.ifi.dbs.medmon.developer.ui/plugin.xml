<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="Data Procesing">
            <command
                  commandId="de.lmu.ifi.dbs.medmon.developer.ui.NewDataProcessingUnit"
                  label="New Data Processing List"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  label="New Wizard"
                  style="push">
            </command>
            <command
                  commandId="de.lmu.ifi.dbs.medmon.developer.ui.LoadDPU"
                  label="Load DPU"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUFormEditor"
            default="false"
            id="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUFormEditor"
            name="DPU Form Editor">
      </editor>
      <editor
            class="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUEditor"
            id="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUEditor"
            name="DPU Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="de.lmu.ifi.dbs.medmon.developer.ui.view.DataView"
            id="de.lmu.ifi.dbs.medmon.developer.ui.dataView"
            name="DataView"
            restorable="true">
      </view>
      <view
            class="de.lmu.ifi.dbs.medmon.developer.ui.view.DPUView"
            id="de.lmu.ifi.dbs.medmon.developer.ui.view.DPUView"
            name="DPU View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.lmu.ifi.dbs.medmon.developer.ui.perspective.PerspectiveFactory"
            id="de.lmu.ifi.dbs.medmon.developer.perspective"
            name="Developer Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="de.lmu.ifi.dbs.medmon.developer.perspective">
         <view
               id="de.lmu.ifi.dbs.medmon.developer.ui.dataView"
               minimized="false"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.lmu.ifi.dbs.medmon.developer.ui.handler.NewDataProcessingUnitCommand"
            id="de.lmu.ifi.dbs.medmon.developer.ui.NewDataProcessingUnit"
            name="New Data Processing Unit">
      </command>
      <command
            defaultHandler="de.lmu.ifi.dbs.medmon.developer.ui.handler.ClusterWizardHandler"
            id="de.lmu.ifi.dbs.medmon.developer.ui.ClusterWizardCommand"
            name="Clustering Wizard">
      </command>
      <command
            defaultHandler="de.lmu.ifi.dbs.medmon.developer.ui.handler.LoadDPUHandler"
            id="de.lmu.ifi.dbs.medmon.developer.ui.LoadDPU"
            name="Load DPU">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            class="de.lmu.ifi.dbs.medmon.developer.ui.wizard.ClusterWizard"
            id="de.lmu.ifi.dbs.medmon.developer.ui.ClusteringWizard"
            name="New Cluster">
      </wizard>
      <wizard
            class="de.lmu.ifi.dbs.medmon.developer.ui.wizard.DPUWizard"
            finalPerspective="de.lmu.ifi.dbs.medmon.developer.perspective"
            id="de.lmu.ifi.dbs.medmon.developer.ui.dpu"
            name="New DPU">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="de.lmu.ifi.dbs.medmon.datamining.core.DPULaunchConfigurationType"
            icon="icons/dpu.png"
            id="de.lmu.ifi.dbs.medmon.developer.ui.DPUlaunchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="de.lmu.ifi.dbs.medmon.developer.ui.launch.DPULaunchConfigurationTabGroup"
            id="de.lmu.ifi.dbs.medmon.developer.ui.DPUlaunchConfigurationTabGroup"
            type="de.lmu.ifi.dbs.medmon.datamining.core.DPULaunchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
      <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUEditor">
         <propertyCategory
               category="general">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUEditor">
         <propertyTab
               afterTab="de.lmu.ifi.dbs.medmon.datamining.core.ParameterPropertyTab"
               category="general"
               id="de.lmu.ifi.dbs.medmon.datamining.core.GeneralPropertyTab"
               label="General">
         </propertyTab>
         <propertyTab
               category="general"
               id="de.lmu.ifi.dbs.medmon.datamining.core.ParameterPropertyTab"
               label="Parameter">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="de.lmu.ifi.dbs.medmon.developer.ui.editor.DPUEditor">
         <propertySection
               class="de.lmu.ifi.dbs.medmon.developer.ui.adaptable.DataProcessorGeneralSection"
               id="de.lmu.ifi.dbs.medmon.datamining.core.DataProcessorGeneralSection"
               tab="de.lmu.ifi.dbs.medmon.datamining.core.GeneralPropertyTab">
            <input
                  type="de.lmu.ifi.dbs.medmon.datamining.core.property.DataProcessorElement">
            </input>
         </propertySection>
      </propertySections>
   </extension>

</plugin>
