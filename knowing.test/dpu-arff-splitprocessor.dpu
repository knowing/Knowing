<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataProcessingUnit name="arff-cross-validation">
    <description>Cross Validation with NaiveBayes output to TablePresenter</description>
    <tags>Cross, Validation, Cross Validation, ARFF, NaiveBayes</tags>
    <nodes>
        <node nodeType="processor" factoryId="knowing.test.processor.SplitProcessor" id="splitter">
            <properties>
                <item value="0.5" key="ratio"/>
            </properties>
        </node>
        <node nodeType="loader" factoryId="weka.core.converters.ArffLoader" id="ARFF">
            <properties>
                <item value="iris.arff" key="file"/>
            </properties>
        </node>
        <node nodeType="presenter" factoryId="de.lmu.ifi.dbs.knowing.core.swt.TablePresenter" id="TablePresenter1">
            <properties>
                <item value="100" key="rows"/>
            </properties>
        </node>
        <node nodeType="presenter" factoryId="de.lmu.ifi.dbs.knowing.core.swt.TablePresenter" id="TablePresenter2">
            <properties>
                <item value="100" key="rows"/>
            </properties>
        </node>
    </nodes>
    <edges>
        <edge weight="1" targetId="splitter" sourceId="ARFF" id="arff_validator"/>
		<edge weight="1" targetId="TablePresenter1" sourceId="splitter:output1" id="validator_presenter"/>
		<edge weight="1" targetId="TablePresenter2" sourceId="splitter:output2" id="validator_presenter"/>
    </edges>
</DataProcessingUnit>

