<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataProcessingUnit name="java-wrapper-test">
    <description>Cross Validation with NaiveBayes output to TablePresenter</description>
    <tags>Cross, Validation, Cross Validation, ARFF, NaiveBayes</tags>
    <nodes>
        <node nodeType="processor" factoryId="knowing.test.processor.TestJavaProcessor" id="java">
            <properties>
                <item value="0.5" key="ratio"/>
            </properties>
        </node>
        <node nodeType="loader" factoryId="weka.core.converters.ArffLoader" id="ARFF">
            <properties>
                <item value="iris.arff" key="file"/>
            </properties>
        </node>
    </nodes>
    <edges>
        <edge weight="1" targetId="java" sourceId="ARFF" id="arff_validator"/>
    </edges>
</DataProcessingUnit>

