<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.lmu.ifi.dbs.knowing.ui.editor.DPUEditor"
            contributorClass="de.lmu.ifi.dbs.knowing.ui.editor.DPUActionBarContributor"
            default="true"
            extensions="dpu, xml"
            filenames="dpu, dpu.xml"
            id="de.lmu.ifi.dbs.knowing.ui.editor.DPUEditor"
            name="DPU Edtior">
      </editor>
      <editor
            class="de.lmu.ifi.dbs.knowing.ui.editor.DPUSapphireEditor"
            default="true"
            extensions="dpu, xml"
            id="de.lmu.ifi.dbs.knowing.ui.editor"
            name="DPU Sapphire Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.lmu.ifi.dbs.knowing.ui.knowing"
            name="Knowing">
      </category>
      <wizard
            category="de.lmu.ifi.dbs.knowing.ui.knowing"
            class="de.lmu.ifi.dbs.knowing.ui.wizard.NewDPUWizard"
            icon="icons/dpu_16.png"
            id="de.lmu.ifi.dbs.knowing.ui.NewDPUWizard"
            name="New DataProcessingUnit">
      </wizard>
      <wizard
            category="de.lmu.ifi.dbs.knowing.ui.knowing"
            class="de.lmu.ifi.dbs.knowing.ui.wizard.NewProcessorWizard"
            id="de.lmu.ifi.dbs.knowing.ui.NewProcessorWizard"
            name="New Processor">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.lmu.ifi.dbs.knowing.core.graph.Node"
            class="de.lmu.ifi.dbs.knowing.ui.adapter.NodeAdapterFactory">
         <adapter
               type="de.lmu.ifi.dbs.knowing.ui.adapter.IWorkbenchColumnAdapter">
         </adapter>
         <adapter
               type="org.eclipse.ui.model.IWorkbenchAdapter">
         </adapter>
      </factory>
      <factory
            adaptableType="de.lmu.ifi.dbs.knowing.core.graph.Edge"
            class="de.lmu.ifi.dbs.knowing.ui.adapter.EdgeAdapterFactory">
         <adapter
               type="de.lmu.ifi.dbs.knowing.ui.adapter.IWorkbenchColumnAdapter">
         </adapter>
         <adapter
               type="org.eclipse.ui.model.IWorkbenchAdapter">
         </adapter>
      </factory>
      <factory
            adaptableType="de.lmu.ifi.dbs.knowing.core.graph.xml.Property"
            class="de.lmu.ifi.dbs.knowing.ui.adapter.PropertyAdapterFactory">
         <adapter
               type="de.lmu.ifi.dbs.knowing.ui.adapter.IWorkbenchColumnAdapter">
         </adapter>
         <adapter
               type="org.eclipse.ui.model.IWorkbenchAdapter">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.lmu.ifi.dbs.knowing.ui.handler.DPUSapphireWizardHandler"
            id="de.lmu.ifi.dbs.knowing.ui.dpu.wizard"
            name="New Sapphire DPU">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <command
               commandId="de.lmu.ifi.dbs.knowing.ui.dpu.wizard"
               label="Sapphire DPU Wizard"
               style="push">
         </command>
      </menuContribution>
   </extension>
</plugin>
